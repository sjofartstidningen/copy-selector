{
  "manifest_version": 2,
  "name": "Copy Selector",
  "version": "1.0",
  "description": "Copy inner html of a selector",
  "content_scripts": [
    {
      "matches": ["http://www.sjofartstidningen.se/?post_type=newsletter*"],
      "run_at": "document_idle",
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.sjofartstidningen.se/?post_type=newsletter*"
  ]
}
